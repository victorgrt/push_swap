################################################################################
# ! ARGUMENTS ! #

NAME	=	push_swap
NAME_BONUS = checker2
CCFLAGS	=	-g -Wall -Werror -Wextra
CC		=	gcc

################################################################################
# * SRC * #

SRC = algo3.c ft_atol.c ft_checker.c parsing.c ft_push.c ft_rotate.c ft_swap.c \
	algo5.c ft_mediane.c ft_print_pile.c ft_pilefind.c \
	ft_check_order.c ft_init_pile.c ft_cmd.c ft_algo_big.c ft_rotate2.c algo_big_utils.c\

SRC_BONUS = get_next_line.c get_next_line_utils.c checker2.c\

OBJ = ${SRC:.c=.o}

OBJ_BONUS = ${SRC_BONUS:.c=.o}

################################################################################
# ? RULES ? #

all: ${NAME} ok_util 

${NAME} : ${OBJ}
	@make -C libft	
	@make -C ft_printf	
	${CC} ${OBJ} libft/libft.a ft_printf/ft_printf.a -o ${NAME}

%.o : %.c
	${CC} ${CCFLAGS} -c $< -o $@

clean:
	@rm -f ${OBJ}

fclean:	clean fclean_util
	@rm -f libft/libft.a
	@rm -f libft/*.o
	@rm -f ft_printf/ft_printf.a
	@rm -f ft_printf/*.o
	@rm -f ${PROG}
	@rm -f ${OBJ}
	@rm -f ${NAME}

re : fclean all

bonus : ${OBJ_BONUS}
	@${CC} ${OBJ_BONUS} ${NAME_BONUS}

.PHONY : all clean fclean all re

################################################################################
# TODO	UTILS	TODO #

fclean_util : 
	@echo "\\033[1;31m DELETED\\033[0;39m"
	@tput sgr0;
	@${foreach value, $(OBJ), echo "\\033[1;31m >> \\033[0;39m" ${value};}
	@${foreach value, $(NAME), echo "\\033[1;31m >> \\033[0;39m" ${value};} 
	@echo "⣿⣷⡶⠚⠉⢀⣤⣾⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⠋⠠⣴⣿⣿⣿⣿⣶⣤⣤⣤\n⠿⠥⢶⡏⣸⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡿⠋⢀⣴⣷⣌⢿⣿⣿⣿⣿⣿⣿⣿\n⣍⡛⢷⣠⣿⣿⣿⣿⣿⣟⠻⣯⠽⣿⣿⠟⠁⣠⠿⠿⣿⣿⣎⠻⣿⣿⣿⡿⠟⣿\n⣿⣿⣦⠙⣿⣿⣿⣿⣿⣿⣷⣏⡧⠙⠁⣀⢾⣧    ⠈⣿⡟  ⠙⣫⣵⣶⠇⣋\n⣿⣿⣿⢀⣿⣿⣿⣿⣿⣿⣿⠟⠃⢀⣀⢻⣎⢻⣷⣤⣴⠟  ⣠⣾⣿⢟⣵⡆⢿\n⣿⣯⣄⢘⢻⣿⣿⣿⣿⡟⠁⢀⣤⡙⢿⣴⣿⣷⡉⠉⢀  ⣴⣿⡿⣡⣿⣿⡿⢆\n⠿⣿⣧⣤⡘⢿⣿⣿⠏  ⡔⠉⠉⢻⣦⠻⣿⣿⣶⣾⡟⣼⣿⣿⣱⣿⡿⢫⣾⣿\n⣷⣮⣝⣛⣃⡉⣿⡏  ⣾⣧⡀    ⣿⡇⢘⣿⠋    ⠻⣿⣿⣿⢟⣵⣿⣿⣿\n⣿⣿⣿⣿⣿⣿⣌⢧⣴⣘⢿⣿⣶⣾⡿⠁⢠⠿⠁⠜    ⣿⣿⣿⣿⡿⣿⣿⣿\n⣿⣿⣿⣿⣿⣿⣿⣦⡙⣿⣷⣉⡛⠋    ⣰⣾⣦⣤⣤⣤⣿⢿⠟⢋⣴⣿⣿⣿\n⣿⣿⣿⣿⣿⣿⣿⣿⣿⣌⢿⣿⣿⣿⣿⢰⡿⣻⣿⣿⣿⣿⣿⢃⣰⣫⣾⣿⣿⣿\n⢿⣿⣿⣿⣿⣿⣿⣿⣿⣿⡆⠿⠿⠿⠛⢰⣾⡿⢟⣭⣿⣿⣿⣿⣿⣿⣿⣿⣿⣿\n"
	
ok_util :
	@echo "\
